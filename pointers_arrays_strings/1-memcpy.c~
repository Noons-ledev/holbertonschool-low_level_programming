#include <string.h>
#include <stdio.h>
#include "main.h"
/**
*_memset- Fills n bytes of memory area 
*@s: Area to process
*@b: Fill with that constant char
*@n: Fill all n bytes
*Return: s
*/
char *_memcpy(char *dest, char *src, unsigned int n)
{
unsigned int a;
if (dest == NULL || src == NULL || n == 0)
return (dest);
for (a = 0; a < n; a++)
{
dest[a] = src[a];
}
return (dest);
}
